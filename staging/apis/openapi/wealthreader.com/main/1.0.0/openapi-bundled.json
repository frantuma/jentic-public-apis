          },
          "yield": {
            "type": "number",
            "example": 256.27
          },
          "contributions": {
            "type": "number",
            "example": 3019.33
          },
          "withholdings": {
            "type": "number",
            "example": 0
          },
          "contracting_date": {
            "type": "string",
            "format": "date",
            "example": "2016-04-27"
          },
          "linked_account": {
            "type": "string",
            "description": "Código IBAN sin espacios. El valor será not-provided cuando la entidad no lo proporcione",
            "example": "ES4021005321592566566159"
          },
          "owners": {
            "$ref": "#/components/schemas/owner"
          },
          "composition": {
            "type": "array",
            "description": "Composición de la cartera",
            "items": {
              "type": "object"
            },
            "properties": {
              "ISIN": {
                "type": "string",
                "description": "Valor indicado siempre que la cartera sea subtype: funds o stocks",
                "example": "ES0152743003"
              },
              "ISIN_description": {
                "type": "string",
                "description": "Valor indicado siempre que la cartera sea subtype: funds o stocks",
                "example": "Fondo Dinámico"
              },
              "DGS": {
                "type": "string",
                "description": "Valor indicado siempre que la cartera sea subtype: pension-plans",
                "example": "F0062"
              },
              "DGS_description": {
                "type": "string",
                "description": "Valor indicado siempre que la cartera sea subtype: pension-plans",
                "example": "ABANCA EMPLEO, FONDO DE PENSIONES"
              },
              "currency": {
                "type": "string",
                "description": "Divisa en codificación ISO 4217",
                "example": "EUR"
              },
              "weighting": {
                "type": "number",
                "description": "Peso con respecto al resto de la composición de la cartera",
                "example": 100
              },
              "shares": {
                "type": "number",
                "description": "Participaciones",
                "example": 225.0077
              },
              "price": {
                "type": "number",
                "description": "Precio actual",
                "example": 14.5577
              },
              "total_amount": {
                "type": "number",
                "description": "Importe total",
                "example": 3275.6
              }
            }
          },
          "transactions": {
            "type": "array",
            "items": {
              "type": "object"
            },
            "required": [
              "uuid",
              "value_date",
              "operation_type",
              "shares",
              "price",
              "total_amount",
              "reference_currency",
              "currency",
              "exchange_rate"
            ],
            "oneOf": [
              {
                "properties": {
                  "ISIN": {
                    "type": "string"
                  }
                }
              },
              {
                "properties": {
                  "DGS": {
                    "type": "string"
                  }
                }
              }
            ],
            "properties": {
              "uuid": {
                "type": "string",
                "description": "Identificador único asignado por Wealth Reader",
                "example": "05e4563f5bbf2d06efd0a29ebe7edbbb4e1bff63"
              },
              "operation_pending": {
                "type": "boolean",
                "description": "Indicado cuando subtype=stocks. Indica si la operación está pendiente o no de ejecutarse.",
                "example": false
              },
              "operation_date": {
                "type": "string",
                "format": "date",
                "example": "2020-01-30"
              },
              "value_date": {
                "type": "string",
                "format": "date",
                "example": "2020-01-30"
              },
              "value_time": {
                "type": "string",
                "description": "Indicado cuando subtype=stocks. La hora de ejecución de la operación.",
                "format": "time",
                "example": "09:00"
              },
              "operation_type": {
                "type": "string",
                "enum": [
                  "purchase",
                  "sale",
                  "subscription by transfer",
                  "redemption by transfer"
                ],
                "example": "purchase"
              },
              "ISIN": {
                "type": "string",
                "description": "Valor indicado siempre que la cartera sea subtype: funds o stocks",
                "example": "ES0152743003"
              },
              "DGS": {
                "type": "string",
                "description": "Valor indicado siempre que la cartera sea subtype: pension-plans",
                "example": "F0062"
              },
              "shares": {
                "type": "number",
                "example": 1076.8273
              },
              "price": {
                "type": "number",
