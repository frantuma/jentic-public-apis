                  }
                }
              },
              "user": {
                "type": "object",
                "properties": {
                  "required": {
                    "type": "boolean",
                    "example": 1,
                    "description": "Indica si el campo es requerido"
                  },
                  "name": {
                    "type": "string",
                    "example": "Nombre de usuario",
                    "description": "Nombre del campo"
                  },
                  "placeholder": {
                    "type": "string",
                    "example": "Email",
